  "description": "My personal Website",
  "main": "index.js",
  "scripts": {
    "deploy": "pm2 deploy ecosystem.config.js production",
    "fix": "./node_modules/.bin/eslint --ext .js --ext .jsx . --fix",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx .",
    "nodemon": "nodemon --inspect server/app.js",
    "precommit": "lint-staged",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "start": "node server/app.js",
    "webpack": "webpack"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --ext .js . --fix",
      "git add"
    ],
    "*.jsx": [
      "./node_modules/.bin/eslint --ext .jsx . --fix",
      "git add"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpbennett4/charlesPaulBennett.git"
  },
